{
	"env": {
		"commonjs": true,
		"node": true
	},
	"extends": "standard",
	"globals": {
		"Atomics": "readable",
		"SharedArrayBuffer": "readable"
	},
	"rules": {
		"indent": [
			"error",
			"tab"
		],
		"no-extra-semi": "off",
		"no-tabs": "off",
		"semi": "off",
		"no-unused-expressions": "off"
	}
}